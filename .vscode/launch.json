{
  "configurations": [
    {
      "name": "k2mult: make all and debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/k2mult.x",
      "args": [
        "web/cnr80k.ck2",
        "web/cnr80k.ck2", "-qvn", "-I", "web/cnr80k.ck2.p" 
      ],
      "stopAtEntry": true,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "make gdb",
      "miDebuggerPath": "/usr/bin/gdb"
    },
